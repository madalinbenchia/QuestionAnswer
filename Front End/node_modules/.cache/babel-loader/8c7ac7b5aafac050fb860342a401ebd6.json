A,GAEA;AACA,kBAAA,MAAA,oBACA,KAAA,CAAA,KAAA,GAAA;AAAA,oBAAA,IAAA,EAAA,KAAA,CAAA;AAAA,mBAAA,GAAA,EADA;AADA,iBAFA;AAAA;AAAA,uBAQA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA,gBAAA,CARA;;AAAA;AAAA;AAAA,yCASA,KAAA,CAAA,MAAA,CAAA,OAAA,CAAA,gBAAA,CATA;;AAAA;AASA,gBAAA,KAAA,CAAA,SATA;AAUA,gBAAA,KAAA,CAAA,QAAA,GAAA,KAAA;AAVA;AAAA;;AAAA;AAAA;AAAA;AAaA,gBAAA,OAAA,CAAA,GAAA;;AACA,gBAAA,KAAA,CAAA,OAAA,CAAA;AACA,kBAAA,IAAA,EAAA,QADA;AAEA,kBAAA,OAAA,EAAA;AAFA,iBAAA;;AAdA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAmBA,KAvBA;AAyBA,IAAA,YAzBA,0BAyBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA,gBAAA,MAFA,GAEA,EAFA;AAAA;AAAA,uBAIA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA,YAAA,CAJA;;AAAA;AAAA;AAAA,yCAKA,MAAA,CAAA,MAAA,CAAA,OAAA,CAAA,YAAA,CALA;;AAAA;AAKA,gBAAA,MAAA,CAAA,KALA;AAMA,gBAAA,MAAA,CAAA,QAAA,GAAA,KAAA;AANA;AAAA;;AAAA;AAAA;AAAA;AAQA,gBAAA,OAAA,CAAA,GAAA;;AACA,gBAAA,MAAA,CAAA,OAAA,CAAA;AACA,kBAAA,IAAA,EAAA,QADA;AAEA,kBAAA,OAAA,EAAA;AAFA,iBAAA;;AATA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAcA,KAvCA;AAyCA,IAAA,YAzCA,wBAyCA,EAzCA,EAyCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,kBAAA,IAAA,EAAA,eADA;AAEA,kBAAA,MAAA,EAAA;AAAA,oBAAA,EAAA,EAAA;AAAA;AAFA,iBAAA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA,KA9CA;AA+CA,IAAA,WA/CA,yBA+CA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA,KAjDA;AAkDA,IAAA,QAlDA,oBAkDA,EAlDA,EAkDA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,kBAAA,IAAA,EAAA,WADA;AAEA,kBAAA,MAAA,EAAA;AAAA,oBAAA,EAAA,EAAA;AAAA;AAFA,iBAAA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA;AAvDA;AArCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"container-fluid mt-5\">\r\n      <div class=\"d-flex justify-content-between\" style=\"margin-right:34%\">\r\n    <h1 class=\"ml-4\">Top Questions</h1>\r\n        <base-button type=\"primary\" icon size=\"lg\" @click=\"addQuestion()\">\r\n          <span class=\"btn-inner--text\">Ask Question</span>\r\n        </base-button>\r\n        </div>\r\n        <div class=\"d-flex justify-content-between\">\r\n          <div class=\"col-8\">\r\n            <div slot=\"empty\" v-if=\"qloading\">\r\n              <img src=\"/img/loading.gif\" style=\"height: 100px; width: 100px\" />\r\n            </div>\r\n            <div v-else>\r\n              <card\r\n                v-for=\"question in this.questions\"\r\n                :key=\"question.id\"\r\n                class=\"no-border-card\"\r\n                body-classes=\"px-4 pb-2\"\r\n                footer-classes=\"pb-2\"\r\n              >\r\n                <h3 slot=\"header\" class=\"h3 mb-0\">{{ question.Title }}</h3>\r\n                 <p v-if =\"question.Body.length > 300\" class=\"card-text mb-4\" v-html=\"question.Body.substring(0,300) + ' ...' \"></p>\r\n                 <p v-else class=\"card-text mb-4\" v-html=\"question.Body\"></p>\r\n                <span\r\n                  v-for=\"tag in question.Tags.split('<')\"\r\n                  :key=\"tag.id\"\r\n                  class=\"badge badge-default mr-1\"\r\n                  >{{ tag.slice(0, -1) }}</span\r\n                >\r\n                <a\r\n                  type=\"text\"\r\n                  @click=\"viewQuestion(question.Id)\"\r\n                  class=\"table-action\"\r\n                  data-toggle=\"tooltip\"\r\n                  style=\"cursor: pointer; float:right\"\r\n                >\r\n                  See more\r\n                </a>\r\n              </card>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"col-4\">\r\n            <div slot=\"empty\" v-if=\"uloading\">\r\n              <img src=\"/img/loading.gif\" style=\"height: 100px; width: 100px\" />\r\n            </div>\r\n            <div\r\n              v-else\r\n              style=\" position: -webkit-sticky;\r\n                      position: sticky;\r\n                      top: 10px;\"\r\n            >\r\n              <card\r\n                gradient=\"success\"\r\n                header-classes=\"bg-transparent\"\r\n                footer-classes=\"bg-transparent\"\r\n                body-classes=\"px-lg-7\"\r\n                class=\"card-pricing border-0 text-center mb-4\"\r\n              >\r\n                <h3 slot=\"header\" class=\"h3 mb-0\" style=\"text-align:center;\">\r\n                  Top Users <i class=\"fas fa-trophy\"></i>\r\n                </h3>\r\n                <p\r\n                  class=\"card-text mb-4\"\r\n                  v-for=\"(user, index) in this.users\"\r\n                  :key=\"user.id\"\r\n                  style=\" white-space: nowrap;\"\r\n                >\r\n                  <b>#{{ Number(index) + 1 }} </b>\r\n                  <b @click=\"viewUser(user.UserId)\" style=\"cursor:pointer\">\r\n                    <i clas=\"mr-1\">  {{ user.DisplayName }} </i></b\r\n                  >\r\n                 : {{ user.Reputation }}  <i class=\"fas fa-star\"></i>\r\n                </p>\r\n              </card>\r\n            </div>\r\n          </div>\r\n        </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { BasePagination } from \"@/components\";\r\nimport StatsCard from \"@/components/Cards/StatsCard\";\r\nimport {\r\n  Table,\r\n  TableColumn,\r\n  DropdownMenu,\r\n  DropdownItem,\r\n  Dropdown,\r\n  Tooltip,\r\n  Select,\r\n  Option,\r\n  Button\r\n} from \"element-ui\";\r\nimport swal from \"sweetalert2\";\r\nimport \"sweetalert2/dist/sweetalert2.css\";\r\n\r\nexport default {\r\n  layout: \"DashboardLayout\",\r\n\r\n  components: {\r\n    StatsCard,\r\n    BasePagination,\r\n    [Tooltip.name]: Tooltip,\r\n    [Table.name]: Table,\r\n    [TableColumn.name]: TableColumn,\r\n    [Dropdown.name]: Dropdown,\r\n    [DropdownItem.name]: DropdownItem,\r\n    [DropdownMenu.name]: DropdownMenu,\r\n    [Select.name]: Select,\r\n    [Option.name]: Option,\r\n    [Button.name]: Button\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      questions: [],\r\n      users: [],\r\n      qloading: true,\r\n      uloading: true,\r\n            query: null,\r\n    };\r\n  },\r\n    watch: {\r\n    query: {\r\n      handler: \"getListDebounced\",\r\n      immediate: true,\r\n    }\r\n    },\r\n\r\n  created() {\r\n    this.getQuestionsList();\r\n    this.getUsersList();\r\n  },\r\n  methods: {\r\n        getListDebounced: _.debounce(function () {\r\n      this.getQuestionsList();\r\n    }, 300),\r\n    async getQuestionsList() {\r\n      try {\r\n        let params = {\r\n          filter: {\r\n            ...(this.query ? { Body: this.query } : {}),\r\n          }\r\n        };\r\n\r\n        await this.$store.dispatch(\"questions/list\");\r\n        this.questions = await { ...this.$store.getters[\"questions/list\"] };\r\n        this.qloading = false;\r\n    \r\n      } catch (error) {\r\n        console.log(error);\r\n        this.$notify({\r\n          type: \"danger\",\r\n          message: \"Oops, something went wrong!\"\r\n        });\r\n      }\r\n    },\r\n\r\n    async getUsersList() {\r\n      try {\r\n        let params = {};\r\n\r\n        await this.$store.dispatch(\"users/list\");\r\n        this.users = await { ...this.$store.getters[\"users/list\"] };\r\n        this.uloading = false;\r\n      } catch (error) {\r\n        console.log(error);\r\n        this.$notify({\r\n          type: \"danger\",\r\n          message: \"Oops, something went wrong!\"\r\n        });\r\n      }\r\n    },\r\n\r\n    async viewQuestion(Id) {\r\n      this.$router.push({\r\n        name: \"View Question\",\r\n        params: { id: Id }\r\n      });\r\n    },\r\n    addQuestion() {\r\n      this.$router.push({ name: \"Add Question\" });\r\n    },\r\n     async viewUser(Id) {\r\n      this.$router.push({\r\n        name: \"View User\",\r\n        params: { id: Id }\r\n      });\r\n    },\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/Examples/QuestionManagement"}]}