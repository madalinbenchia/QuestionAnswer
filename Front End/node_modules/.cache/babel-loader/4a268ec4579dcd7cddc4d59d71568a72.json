CAAA;AACA,kBAAA,IAAA,EAAA,QADA;AAEA,kBAAA,OAAA,EAAA;AAFA,iBAAA,CARA;;AAAA;AAYA,gBAAA,KAAA,CAAA,gBAAA,CAAA,YAAA,QAAA,CAAA,IAAA,CAAA,MAAA;;AAZA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAcA;AAfA;AAjBA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- Header -->\r\n    <div class=\"header bg-gradient-success py-7 py-lg-8 pt-lg-9\">\r\n      <div class=\"container\">\r\n        <notifications></notifications>\r\n      </div>\r\n      <div class=\"separator separator-bottom separator-skew zindex-100\">\r\n        <svg\r\n          x=\"0\"\r\n          y=\"0\"\r\n          viewBox=\"0 0 2560 100\"\r\n          preserveAspectRatio=\"none\"\r\n          version=\"1.1\"\r\n          xmlns=\"http://www.w3.org/2000/svg\"\r\n        >\r\n          <polygon\r\n            class=\"fill-default\"\r\n            points=\"2560 0 2560 100 0 100\"\r\n          ></polygon>\r\n        </svg>\r\n      </div>\r\n    </div>\r\n    <!-- Page content -->\r\n    <div class=\"container mt--9 pb-5\">\r\n      <div class=\"row justify-content-center\">\r\n        <div class=\"col-lg-5 col-md-7\">\r\n          <div class=\"card bg-secondary border-0 mb-0\">\r\n            <div class=\"card-body px-lg-5 py-lg-5\">\r\n              <div class=\"text-center text-muted mb-4\">\r\n                <small>Reset password</small>\r\n              </div>\r\n\r\n              <form class=\"needs-validation\" @submit.prevent=\"handleSubmit()\">\r\n                <base-input\r\n                  alternative\r\n                  class=\"mb-3\"\r\n                  name=\"Email\"\r\n                  prepend-icon=\"ni ni-email-83\"\r\n                  placeholder=\"Email\"\r\n                  v-model=\"form.data.attributes.email\"\r\n                >\r\n                </base-input>\r\n                <validation-error :errors=\"apiValidationErrors.email\" />\r\n\r\n                <div class=\"text-center\">\r\n                  <!-- <base-button type=\"submit\" @click.prevent=\"handleSubmit\" class=\"my-4\">Sign in</base-button> -->\r\n                  <base-button type=\"primary\" native-type=\"submit\" class=\"my-4\"\r\n                    >Send Password Reset Link</base-button\r\n                  >\r\n                </div>\r\n              </form>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport ValidationError from \"@/components/ValidationError.vue\";\r\nimport formMixin from \"@/mixins/form-mixin\";\r\nexport default {\r\n  layout: \"AuthLayout\",\r\n  mixins: [formMixin],\r\n  components: { ValidationError },\r\n  data() {\r\n    return {\r\n      form: {\r\n        data: {\r\n          type: \"password-forgot\",\r\n          attributes: {\r\n            email: \"\",\r\n            redirect_url: process.env.VUE_APP_BASE_URL + \"/password/email\",\r\n          },\r\n        },\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    async handleSubmit() {\r\n      try {\r\n        await this.$store.dispatch(\"reset/forgotPassword\", this.form.data);\r\n        await this.$notify({\r\n          type: \"success\",\r\n          message: \"An email with reset password link was sent.\",\r\n        });\r\n      } catch (error) {\r\n        await this.$notify({\r\n          type: \"danger\",\r\n          message: \"We can't find a user with that e-mail address.\",\r\n        });\r\n        this.setApiValidation(error.response.data.errors);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/Password"}]}