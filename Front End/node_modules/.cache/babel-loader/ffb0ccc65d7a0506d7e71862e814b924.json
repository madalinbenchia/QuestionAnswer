,EAAA,EAAA,CAdA;;AAAA;AAAA;AAAA,uBAeA,MAAA,CAAA,OAAA,EAfA;;AAAA;AAgBA,gBAAA,MAAA,CAAA,OAAA,CAAA;AACA,kBAAA,IAAA,EAAA,SADA;AAEA,kBAAA,OAAA,EAAA;AAFA,iBAAA;;AAhBA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAsBA,gBAAA,MAAA,CAAA,OAAA,CAAA;AACA,kBAAA,IAAA,EAAA,QADA;AAEA,kBAAA,OAAA,EACA;AAHA,iBAAA;;AAtBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA4BA,KAjDA;AAkDA;AACA;AACA;AACA,IAAA,QArDA,oBAqDA,GArDA,EAqDA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,WADA;AAEA,QAAA,MAAA,EAAA;AAAA,UAAA,EAAA,EAAA,GAAA,CAAA;AAAA;AAFA,OAAA;AAIA,KA1DA;AA2DA,IAAA,UA3DA,4BA2DA;AAAA,UAAA,IAAA,QAAA,IAAA;AAAA,UAAA,KAAA,QAAA,KAAA;;AACA,UAAA,KAAA,KAAA,YAAA,EAAA;AACA,aAAA,IAAA,cAAA,IAAA;AACA,OAFA,MAEA;AACA,aAAA,IAAA,aAAA,IAAA;AACA;;AACA,WAAA,OAAA;AACA;AAlEA;AAlEA,CAAA","sourcesContent":["<template>\r\n  <div class=\"container-fluid mt-5\">\r\n    <div>\r\n      <card\r\n        class=\"no-border-card\"\r\n        body-classes=\"px-0 pb-1\"\r\n        footer-classes=\"pb-2\"\r\n      >\r\n        <template slot=\"header\">\r\n          <div class=\"row\">\r\n            <div class=\"col-6\">\r\n              <h3 class=\"mb-0\">Roles List</h3>\r\n            </div>\r\n            <div class=\"col-6 text-right\">\r\n              <!-- <base-button type=\"primary\" icon size=\"sm\" @click=\"addRole()\">\r\n                <span class=\"btn-inner--icon\"\r\n                  ><i class=\"fas fa-user-edit\"></i\r\n                ></span>\r\n                <span class=\"btn-inner--text\">Add Role</span>\r\n              </base-button> -->\r\n            </div>\r\n          </div>\r\n        </template>\r\n        <div>\r\n          <div\r\n            class=\"col-12 d-flex justify-content-center justify-content-sm-between flex-wrap\"\r\n          >\r\n            <el-select\r\n              class=\"select-primary pagination-select\"\r\n              v-model=\"pagination.perPage\"\r\n              placeholder=\"Per page\"\r\n            >\r\n              <el-option\r\n                class=\"select-primary\"\r\n                v-for=\"item in pagination.perPageOptions\"\r\n                :key=\"item\"\r\n                :label=\"item\"\r\n                :value=\"item\"\r\n              >\r\n              </el-option>\r\n            </el-select>\r\n\r\n            <div>\r\n              <base-input\r\n                v-model=\"query\"\r\n                type=\"search\"\r\n                prepend-icon=\"fas fa-search\"\r\n                placeholder=\"Search...\"\r\n                clearable\r\n              />\r\n            </div>\r\n          </div>\r\n          <el-table\r\n            class=\"table-responsive align-items-center table-flush\"\r\n            header-row-class-name=\"thead-light\"\r\n            :data=\"roles\"\r\n            @sort-change=\"sortChange\"\r\n          >\r\n            <el-table-column label=\"Name\" prop=\"name\" sortable=\"custom\" />\r\n            <el-table-column\r\n              label=\"Created At\"\r\n              prop=\"created_at\"\r\n              sortable=\"custom\"\r\n            />\r\n            <el-table-column align=\"center\">\r\n              <div slot-scope=\"{ row }\" class=\"table-actions\">\r\n                <el-tooltip content=\"Edit\" placement=\"top\">\r\n                  <a\r\n                    type=\"text\"\r\n                    @click=\"editRole(row)\"\r\n                    class=\"table-action\"\r\n                    data-toggle=\"tooltip\"\r\n                  >\r\n                    <i class=\"fas fa-user-edit\" />\r\n                  </a>\r\n                </el-tooltip>\r\n\r\n                <el-tooltip content=\"Delete\" placement=\"top\">\r\n                  <a\r\n                    type=\"text\"\r\n                    @click=\"deleteRole(row.id)\"\r\n                    class=\"table-action table-action-delete\"\r\n                    data-toggle=\"tooltip\"\r\n                  >\r\n                    <i class=\"fas fa-trash\" />\r\n                  </a>\r\n                </el-tooltip>\r\n              </div>\r\n            </el-table-column>\r\n          </el-table>\r\n        </div>\r\n        <div\r\n          slot=\"footer\"\r\n          class=\"col-12 d-flex justify-content-center justify-content-sm-between flex-wrap\"\r\n        >\r\n          <div class=\"\">\r\n            <p class=\"card-category\">\r\n              Showing {{ from + 1 }} to {{ to }} of {{ total }} entries\r\n\r\n              <span v-if=\"selectedRows.length\">\r\n                &nbsp; &nbsp; {{ selectedRows.length }} rows selected\r\n              </span>\r\n            </p>\r\n          </div>\r\n          <base-pagination\r\n            class=\"pagination-no-border\"\r\n            v-model=\"pagination.currentPage\"\r\n            :per-page=\"pagination.perPage\"\r\n            :total=\"total\"\r\n          />\r\n        </div>\r\n      </card>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { BasePagination } from \"@/components/\";\r\nimport {\r\n  Table,\r\n  TableColumn,\r\n  DropdownMenu,\r\n  DropdownItem,\r\n  Dropdown,\r\n  Tooltip,\r\n  Select,\r\n  Option,\r\n  RadioButton,\r\n  Button\r\n} from \"element-ui\";\r\nimport swal from \"sweetalert2\";\r\nimport \"sweetalert2/dist/sweetalert2.css\";\r\n\r\nexport default {\r\n  layout: \"DashboardLayout\",\r\n\r\n  components: {\r\n    BasePagination,\r\n    [Tooltip.name]: Tooltip,\r\n    [Table.name]: Table,\r\n    [TableColumn.name]: TableColumn,\r\n    [Dropdown.name]: Dropdown,\r\n    [DropdownItem.name]: DropdownItem,\r\n    [DropdownMenu.name]: DropdownMenu,\r\n    [Select.name]: Select,\r\n    [Option.name]: Option,\r\n    [Button.name]: Button\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      roles: [],\r\n      selectedRows: [],\r\n\r\n      query: null,\r\n\r\n      sort: \"created_at\",\r\n\r\n      pagination: {\r\n        perPage: 5,\r\n        currentPage: 1,\r\n        perPageOptions: [5, 10, 25, 50]\r\n      },\r\n\r\n      total: 0\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    from() {\r\n      return this.pagination.perPage * (this.pagination.currentPage - 1);\r\n    },\r\n\r\n    to() {\r\n      let highBound = this.from + this.pagination.perPage;\r\n      if (this.total < highBound) {\r\n        highBound = this.total;\r\n      }\r\n      return highBound;\r\n    }\r\n  },\r\n\r\n  watch: {\r\n    query: {\r\n      handler: \"getListDebounced\",\r\n      immediate: true\r\n    },\r\n    pagination: {\r\n      handler: \"getList\",\r\n      immediate: false,\r\n      deep: true\r\n    }\r\n  },\r\n  async fetch() {\r\n    await this.$store.dispatch(\"roles/list\", params);\r\n    this.roles = this.$store.getters[\"roles/list\"];\r\n    this.total = this.$store.getters[\"roles/listTotal\"];\r\n  },\r\n\r\n  methods: {\r\n    getListDebounced: _.debounce(function() {\r\n      this.getList();\r\n    }, 300),\r\n\r\n    async getList() {\r\n      let params = {\r\n        ...(this.sort ? { sort: this.sort } : {}),\r\n        filter: {\r\n          ...(this.query ? { name: this.query } : {})\r\n        },\r\n        page: {\r\n          number: this.pagination.currentPage,\r\n          size: this.pagination.perPage\r\n        }\r\n      };\r\n      await this.$store.dispatch(\"roles/list\", params);\r\n      this.roles = this.$store.getters[\"roles/list\"];\r\n      this.total = this.$store.getters[\"roles/listTotal\"];\r\n    },\r\n\r\n    async deleteRole(id) {\r\n      const confirmation = await swal.fire({\r\n        title: `Delete this role?`,\r\n        type: \"question\",\r\n        buttonsStyling: false,\r\n        showCancelButton: true,\r\n        confirmButtonText: \"Yes, delete it!\",\r\n        cancelButtonText: \"No, keep it\",\r\n        confirmButtonClass: \"btn btn-primary\",\r\n        cancelButtonClass: \"btn btn-warning\"\r\n      });\r\n\r\n      try {\r\n        if (confirmation.value === true) {\r\n          await this.$store.dispatch(\"roles/destroy\", id);\r\n          await this.getList();\r\n          this.$notify({\r\n            type: \"success\",\r\n            message: \"Role deleted successfully.\"\r\n          });\r\n        }\r\n      } catch (error) {\r\n        this.$notify({\r\n          type: \"danger\",\r\n          message:\r\n            \"Cannot delete this role because the record has associated users.\"\r\n        });\r\n      }\r\n    },\r\n    // addRole() {\r\n    //   this.$router.push({ name: \"examples-role-management-add-role\" });\r\n    // },\r\n    editRole(row) {\r\n      this.$router.push({\r\n        name: \"Edit Role\",\r\n        params: { id: row.id }\r\n      });\r\n    },\r\n    sortChange({ prop, order }) {\r\n      if (order === \"descending\") {\r\n        this.sort = `-${prop}`;\r\n      } else {\r\n        this.sort = `${prop}`;\r\n      }\r\n      this.getList();\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/Examples"}]}