AAA,uBAIA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA,aAAA,EAAA,EAAA,CAJA;;AAAA;AAKA,gBAAA,KAAA,CAAA,MAAA,GAAA,KAAA,CAAA,MAAA,CAAA,OAAA,CAAA,gBAAA,CAAA;AAEA,gBAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AAPA;AAAA;;AAAA;AAAA;AAAA;AASA,gBAAA,OAAA,CAAA,GAAA;;AACA,gBAAA,KAAA,CAAA,OAAA,CAAA;AACA,kBAAA,IAAA,EAAA,QADA;AAEA,kBAAA,OAAA;AAFA,iBAAA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAeA,KAhBA;AAkBA,IAAA,MAlBA,oBAkBA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,eADA;AAEA,QAAA,MAAA,EAAA;AAAA,UAAA,EAAA,EAAA,KAAA,MAAA,CAAA;AAAA;AAFA,OAAA;AAIA,KAvBA;AAyBA,IAAA,YAzBA,0BAyBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,yCAEA,MAAA,CAAA,MAAA,CAAA,OAAA,CAAA,WAFA;;AAAA;AAEA,gBAAA,MAAA,CAAA,IAFA;AAGA,gBAAA,MAAA,CAAA,MAAA,CAAA,YAAA,GAAA,MAAA,GAAA,MAAA,EAAA;AACA,gBAAA,MAAA,CAAA,MAAA,CAAA,gBAAA,GAAA,MAAA,GAAA,MAAA,EAAA;AACA,gBAAA,MAAA,CAAA,MAAA,CAAA,SAAA,GAAA,MAAA,GAAA,MAAA,EAAA;AACA,gBAAA,MAAA,CAAA,MAAA,CAAA,IAAA,GAAA,EAAA;AACA,gBAAA,MAAA,CAAA,MAAA,CAAA,KAAA,GAAA,EAAA;AAEA,gBAAA,MAAA,CAAA,MAAA,CAAA,qBAAA,GAAA,MAAA,CAAA,IAAA,CAAA,WAAA;AATA;AAAA,uBAWA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA,gBAAA,EAAA,MAAA,CAAA,MAAA,CAXA;;AAAA;AAAA;AAAA,uBAYA,MAAA,CAAA,MAAA,CAAA,OAAA,CAAA,gBAAA,CAZA;;AAAA;AAYA,gBAAA,MAAA,CAAA,MAZA;AAaA,gBAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA,MAAA;;AACA,gBAAA,MAAA,CAAA,OAAA,CAAA;AACA,kBAAA,IAAA,EAAA,SADA;AAEA,kBAAA,OAAA,EAAA;AAFA,iBAAA;;AAIA,gBAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,kBAAA,IAAA,EAAA,eADA;AAEA,kBAAA,MAAA,EAAA;AAAA,oBAAA,EAAA,EAAA,MAAA,CAAA,MAAA,CAAA;AAAA;AAFA,iBAAA;;AAlBA;AAAA;;AAAA;AAAA;AAAA;;AAuBA,gBAAA,MAAA,CAAA,OAAA,CAAA;AACA,kBAAA,IAAA,EAAA,QADA;AAEA,kBAAA,OAAA,EAAA;AAFA,iBAAA;;AAvBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA4BA;AArDA;AA7BA,CAAA","sourcesContent":["<template>\r\n  <div class=\"container-fluid mt-5\">\r\n    <div class=\"row\">\r\n      <div class=\"col-xl-12 order-xl-1\">\r\n        <card>\r\n          <div slot=\"header\" class=\"row align-items-center\">\r\n            <div class=\"col-8\">\r\n              <h3 class=\"mb-0\">Edit Answer</h3>\r\n            </div>\r\n            <div class=\"col-4 text-right\">\r\n              <base-button\r\n                @click=\"goBack\"\r\n                type=\"button\"\r\n                class=\"btn btn-sm btn-primary\"\r\n                >Go Back</base-button\r\n              >\r\n            </div>\r\n          </div>\r\n          <div class=\"card-body\">\r\n            <form ref=\"profile_form\" @submit.prevent=\"handleSubmit\">\r\n              <p\r\n                style=\"color: darkgray;margin-top: -10px;margin-bottom: 10px;font-style: italic;\"\r\n              >\r\n                Please be sure to answer the question. Provide details and share\r\n                your research!\r\n              </p>\r\n              <!-- <html-editor v-model=\"question.body\" name=\"editor\" /> -->\r\n              <vue-editor\r\n                v-model=\"answer.Body\"\r\n                :editorToolbar=\"customToolbar\"\r\n              ></vue-editor>\r\n\r\n              <base-button\r\n                type=\"button\"\r\n                class=\"btn btn-sm btn-primary mt-4 ml-0\"\r\n                native-type=\"submit\"\r\n              >\r\n                Edit answer\r\n              </base-button>\r\n            </form>\r\n          </div>\r\n        </card>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport BaseInput from \"@/components/Inputs/BaseInput.vue\";\r\nimport { Select, Option } from \"element-ui\";\r\nimport HtmlEditor from \"@/components/Inputs/HtmlEditor\";\r\nimport InputTag from \"vue-input-tag\";\r\nimport { VueEditor } from \"vue2-editor\";\r\nimport moment from \"moment\";\r\n\r\nexport default {\r\n  layout: \"DashboardLayout\",\r\n  components: {\r\n    BaseInput,\r\n    HtmlEditor,\r\n    [Select.name]: Select,\r\n    [Option.name]: Option,\r\n    InputTag,\r\n    VueEditor\r\n  },\r\n  data() {\r\n    return {\r\n      customToolbar: [\r\n        [\"bold\", \"italic\", \"underline\"],\r\n        [{ list: \"ordered\" }, { list: \"bullet\" }],\r\n        [\"image\", \"code\"]\r\n      ],\r\n      user: null,\r\n      answer: {\r\n        body: \"\",\r\n        owneruserid: \"\"\r\n      }\r\n    };\r\n  },\r\n\r\n  created() {\r\n    this.get();\r\n  },\r\n\r\n  methods: {\r\n    async get() {\r\n      try {\r\n        this.user = await { ...this.$store.getters.currentUser };\r\n        const id = this.$route.params.id;\r\n        await this.$store.dispatch(\"answers/get\", id);\r\n        this.answer = this.$store.getters[\"answers/answer\"];\r\n\r\n        this.loading = false;\r\n      } catch (error) {\r\n        console.log(error);\r\n        this.$notify({\r\n          type: \"danger\",\r\n          message: `Oops, something went wrong!`\r\n        });\r\n      }\r\n    },\r\n\r\n    goBack() {\r\n      this.$router.push({\r\n        name: \"View Question\",\r\n        params: { id: this.answer.ParentId }\r\n      });\r\n    },\r\n\r\n    async handleSubmit() {\r\n      try {\r\n        this.user = await { ...this.$store.getters.currentUser };\r\n        this.answer.LastEditDate = moment().format();\r\n        this.answer.LastActivityDate = moment().format();\r\n        this.answer.CloseDate = moment().format();\r\n        this.answer.Tags = \"\";\r\n        this.answer.Title = \"\";\r\n\r\n        this.answer.LastEditorDisplayName = this.user.DisplayName;\r\n\r\n        await this.$store.dispatch(\"answers/update\", this.answer);\r\n        this.answer = await this.$store.getters[\"answers/answer\"];\r\n        console.log(this.answer);\r\n        this.$notify({\r\n          type: \"success\",\r\n          message: \"Answer edited successfully.\"\r\n        });\r\n        this.$router.push({\r\n          name: \"View Question\",\r\n          params: { id: this.answer.ParentId }\r\n        });\r\n      } catch (error) {\r\n        this.$notify({\r\n          type: \"danger\",\r\n          message: \"Oops, something went wrong!\"\r\n        });\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/Examples/QuestionManagement"}]}