CAAA,KAAA,CAAA,SAAA;AACA,gBAAA,KAAA,CAAA,OAAA,GAAA,KAAA,CANA,CAQA;AACA;;AATA;AAAA;;AAAA;AAAA;AAAA;;AAWA,gBAAA,KAAA,CAAA,OAAA,CAAA;AACA,kBAAA,IAAA,EAAA,QADA;AAEA,kBAAA,OAAA;AAFA,iBAAA;;AAIA,gBAAA,OAAA,CAAA,GAAA;;AAfA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAiBA,KAlBA;AAoBA,IAAA,YApBA,0BAoBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA,aAAA,EAAA,MAAA,CAAA,MAAA,CAFA;;AAAA;AAIA,gBAAA,MAAA,CAAA,OAAA,CAAA;AACA,kBAAA,IAAA,EAAA,SADA;AAEA,kBAAA,OAAA,EAAA;AAFA,iBAAA;;AAIA,gBAAA,MAAA,CAAA,MAAA;;AARA;AAAA;;AAAA;AAAA;AAAA;;AAUA,gBAAA,MAAA,CAAA,OAAA,CAAA;AACA,kBAAA,IAAA,EAAA,QADA;AAEA,kBAAA,OAAA,EAAA;AAFA,iBAAA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAeA,KAnCA;AAqCA,IAAA,MArCA,oBAqCA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA,KAvCA;AAwCA,IAAA,WAxCA,yBAwCA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA,KA1CA;AA2CA,IAAA,YA3CA,wBA2CA,EA3CA,EA2CA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,kBAAA,IAAA,EAAA,eADA;AAEA,kBAAA,MAAA,EAAA;AAAA,oBAAA,EAAA,EAAA;AAAA;AAFA,iBAAA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA;AAhDA;AA9BA,CAAA","sourcesContent":["<template>\r\n  <div class=\"container-fluid mt-5\">\r\n      <div>\r\n      \r\n     \r\n        <div class=\"d-flex justify-content-between\">\r\n          <div class=\"col-12\">\r\n            <div slot=\"empty\" v-if=\"loading\">\r\n              <img src=\"/img/loading.gif\" style=\"height: 100px; width: 100px\" />\r\n            </div>\r\n            <div v-else>\r\n                <div v-if=\"Object.keys(this.questions).length == 0\">  <img src=\"/img/brand/ono.png\" style=\" display: block;\r\n  margin-left: auto;\r\n  margin-right: auto;\" />\r\n\r\n     <base-button style=\" display: block;\r\n  margin-left: auto;\r\n  margin-right: auto; margin-top: 35px\"  type=\"primary\" icon size=\"lg\" @click=\"addQuestion()\">\r\n          <span class=\"btn-inner--icon\"><i class=\"fas fa-user-edit\"></i></span>\r\n          <span class=\"btn-inner--text\">Add Question</span>\r\n        </base-button>\r\n        </div>\r\n              <card\r\n                v-for=\"question in this.questions\"\r\n                :key=\"question.id\"\r\n                class=\"no-border-card\"\r\n                body-classes=\"px-4 pb-2\"\r\n                footer-classes=\"pb-2\"\r\n              >\r\n                <h3 slot=\"header\" class=\"h3 mb-0\">{{ question.Title }}</h3>\r\n                <p v-if =\"question.Body.length > 300\" class=\"card-text mb-4\" v-html=\"question.Body.substring(0,300) + ' ...' \"></p>\r\n                 <p v-else class=\"card-text mb-4\" v-html=\"question.Body\"></p>\r\n                <span\r\n                  v-for=\"tag in question.Tags.split('<')\"\r\n                  :key=\"tag.id\"\r\n                  class=\"badge badge-default mr-1\"\r\n                  >{{ tag.slice(0, -1) }}</span\r\n                >\r\n                <a\r\n                  type=\"text\"\r\n                  @click=\"editQuestion(question.Id)\"\r\n                  class=\"table-action\"\r\n                  data-toggle=\"tooltip\"\r\n                  style=\"cursor: pointer; float:right\"\r\n                >\r\n                  See more\r\n                </a>\r\n              </card>\r\n            </div>\r\n          </div>\r\n\r\n\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport BaseInput from \"@/components/Inputs/BaseInput.vue\";\r\nimport BaseButton from \"@/components/BaseButton.vue\";\r\nimport ValidationError from \"@/components/ValidationError.vue\";\r\nimport { Select, Option } from \"element-ui\";\r\nimport formMixin from \"@/mixins/form-mixin\";\r\nimport moment from \"moment\";\r\nimport HtmlEditor from \"@/components/Inputs/HtmlEditor\";\r\n\r\nexport default {\r\n  layout: \"DashboardLayout\",\r\n\r\n  components: {\r\n    BaseInput,\r\n    BaseButton,\r\n    ValidationError,\r\n    [Select.name]: Select,\r\n    [Option.name]: Option,\r\n    HtmlEditor\r\n  },\r\n  mixins: [formMixin],\r\n\r\n  data() {\r\n    return {\r\n      id: \"\",\r\n      loading: true,\r\n      questions: null,\r\n    };\r\n  },\r\n\r\n  created() {\r\n    this.get();\r\n  },\r\n  watch: {\r\n    $route() {\r\n      this.get();\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    async get() {\r\n      try {\r\n        this.id = this.$route.params.id;\r\n        await this.$store.dispatch(\"questions/search\", this.id);\r\n        this.questions = await { ...this.$store.getters[\"questions/search\"] };\r\n        console.log(this.questions);\r\n        this.loading = false;\r\n\r\n        // this.askedTimeAgo = moment(this.question.CreationDate).fromNow();\r\n        // this.activityTimeAgo = moment(this.question.LastActivityDate).fromNow();\r\n      } catch (error) {\r\n        this.$notify({\r\n          type: \"danger\",\r\n          message: `Oops, something went wrong!`\r\n        });\r\n        console.log(error);\r\n      }\r\n    },\r\n\r\n    async handleSubmit() {\r\n      try {\r\n        await this.$store.dispatch(\"answers/add\", this.answer);\r\n\r\n        this.$notify({\r\n          type: \"success\",\r\n          message: \"Answer added successfully.\"\r\n        });\r\n        this.goBack();\r\n      } catch (error) {\r\n        this.$notify({\r\n          type: \"danger\",\r\n          message: \"Oops, something went wrong!\"\r\n        });\r\n      }\r\n    },\r\n\r\n    goBack() {\r\n      this.$router.push({ name: \"List Questions\" });\r\n    },\r\n    addQuestion() {\r\n      this.$router.push({ name: \"Add Question\" });\r\n    },\r\n    async editQuestion(Id) {\r\n      this.$router.push({\r\n        name: \"View Question\",\r\n        params: { id: Id }\r\n      });\r\n    },\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/Examples/QuestionManagement"}]}