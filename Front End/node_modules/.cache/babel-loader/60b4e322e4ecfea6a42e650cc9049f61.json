    </nav>\r\n        </div>\r\n        <div class=\"col-lg-6 col-5 text-right\">\r\n          <base-button size=\"sm\" type=\"neutral\">New</base-button>\r\n          <base-button size=\"sm\" type=\"neutral\">Filters</base-button>\r\n        </div>\r\n      </div>\r\n      <!-- Card stats -->\r\n      <div class=\"row\">\r\n        <div class=\"col-xl-3 col-md-6\">\r\n          <stats-card title=\"Total traffic\"\r\n                      type=\"gradient-red\"\r\n                      sub-title=\"350,897\"\r\n                      icon=\"ni ni-active-40\">\r\n\r\n            <template slot=\"footer\">\r\n              <span class=\"text-success mr-2\"><i class=\"fa fa-arrow-up\"></i> 3.48%</span>\r\n              <span class=\"text-nowrap\">Since last month</span>\r\n            </template>\r\n          </stats-card>\r\n        </div>\r\n        <div class=\"col-xl-3 col-md-6\">\r\n          <stats-card title=\"Total traffic\"\r\n                      type=\"gradient-orange\"\r\n                      sub-title=\"2,356\"\r\n                      icon=\"ni ni-chart-pie-35\">\r\n\r\n            <template slot=\"footer\">\r\n              <span class=\"text-success mr-2\"><i class=\"fa fa-arrow-up\"></i> 12.18%</span>\r\n              <span class=\"text-nowrap\">Since last month</span>\r\n            </template>\r\n          </stats-card>\r\n        </div>\r\n        <div class=\"col-xl-3 col-md-6\">\r\n          <stats-card title=\"Sales\"\r\n                      type=\"gradient-green\"\r\n                      sub-title=\"924\"\r\n                      icon=\"ni ni-money-coins\">\r\n\r\n            <template slot=\"footer\">\r\n              <span class=\"text-danger mr-2\"><i class=\"fa fa-arrow-down\"></i> 5.72%</span>\r\n              <span class=\"text-nowrap\">Since last month</span>\r\n            </template>\r\n          </stats-card>\r\n\r\n        </div>\r\n        <div class=\"col-xl-3 col-md-6\">\r\n          <stats-card title=\"Performance\"\r\n                      type=\"gradient-info\"\r\n                      sub-title=\"49,65%\"\r\n                      icon=\"ni ni-chart-bar-32\">\r\n\r\n            <template slot=\"footer\">\r\n              <span class=\"text-success mr-2\"><i class=\"fa fa-arrow-up\"></i> 54.8%</span>\r\n              <span class=\"text-nowrap\">Since last month</span>\r\n            </template>\r\n          </stats-card>\r\n        </div>\r\n      </div>\r\n    </base-header>\r\n\r\n    <!--Charts-->\r\n    <div class=\"container-fluid mt--6\">\r\n      <div class=\"row\">\r\n        <div class=\"col-xl-8\">\r\n          <card type=\"default\" header-classes=\"bg-transparent\">\r\n            <div slot=\"header\" class=\"row align-items-center\">\r\n              <div class=\"col\">\r\n                <h6 class=\"text-light text-uppercase ls-1 mb-1\">Overview</h6>\r\n                <h5 class=\"h3 text-white mb-0\">Sales value</h5>\r\n              </div>\r\n              <div class=\"col\">\r\n                <ul class=\"nav nav-pills justify-content-end\">\r\n                  <li class=\"nav-item mr-2 mr-md-0\">\r\n                    <a class=\"nav-link py-2 px-3\"\r\n                       href=\"#\"\r\n                       :class=\"{active: bigLineChart.activeIndex === 0}\"\r\n                       @click.prevent=\"initBigChart(0)\">\r\n                      <span class=\"d-none d-md-block\">Month</span>\r\n                      <span class=\"d-md-none\">M</span>\r\n                    </a>\r\n                  </li>\r\n                  <li class=\"nav-item\">\r\n                    <a class=\"nav-link py-2 px-3\"\r\n                       href=\"#\"\r\n                       :class=\"{active: bigLineChart.activeIndex === 1}\"\r\n                       @click.prevent=\"initBigChart(1)\">\r\n                      <span class=\"d-none d-md-block\">Week</span>\r\n                      <span class=\"d-md-none\">W</span>\r\n                    </a>\r\n                  </li>\r\n                </ul>\r\n              </div>\r\n            </div>\r\n            <line-chart\r\n              :height=\"350\"\r\n              ref=\"bigChart\"\r\n              :chart-data=\"bigLineChart.chartData\"\r\n              :extra-options=\"bigLineChart.extraOptions\"\r\n            >\r\n            </line-chart>\r\n\r\n          </card>\r\n        </div>\r\n\r\n        <div class=\"col-xl-4\">\r\n          <card header-classes=\"bg-transparent\">\r\n            <div slot=\"header\" class=\"row align-items-center\">\r\n              <div class=\"col\">\r\n                <h6 class=\"text-uppercase text-muted ls-1 mb-1\">Performance</h6>\r\n                <h5 class=\"h3 mb-0\">Total orders</h5>\r\n              </div>\r\n            </div>\r\n\r\n            <bar-chart\r\n              :height=\"350\"\r\n              ref=\"barChart\"\r\n              :chart-data=\"redBarChart.chartData\"\r\n            >\r\n            </bar-chart>\r\n          </card>\r\n        </div>\r\n      </div>\r\n      <!-- End charts-->\r\n\r\n      <!--Lists-->\r\n      <div class=\"row\">\r\n        <div class=\"col-xl-4\">\r\n          <card>\r\n            <h5 class=\"h3 mb-0\" slot=\"header\">Team members</h5>\r\n\r\n            <user-list></user-list>\r\n          </card>\r\n        </div>\r\n\r\n        <div class=\"col-xl-4\">\r\n          <card>\r\n            <h5 class=\"h3 mb-0\" slot=\"header\">To do list</h5>\r\n\r\n            <task-list></task-list>\r\n          </card>\r\n        </div>\r\n\r\n        <div class=\"col-xl-4\">\r\n          <card>\r\n            <h5 class=\"h3 mb-0\" slot=\"header\">Progress track</h5>\r\n\r\n            <progress-track-list></progress-track-list>\r\n          </card>\r\n        </div>\r\n      </div>\r\n      <!--End lists-->\r\n\r\n      <!--Widgets-->\r\n      <div class=\"row\">\r\n        <div class=\"col-xl-5\">\r\n          <activity-feed></activity-feed>\r\n        </div>\r\n        <div class=\"col-xl-7\">\r\n          <light-table></light-table>\r\n          <div class=\"card-deck\">\r\n            <card gradient=\"default\" no-body=\"\">\r\n              <div class=\"card-body\">\r\n                <div class=\"mb-2\">\r\n                  <sup class=\"text-white\">$</sup> <span class=\"h2 text-white\">3,300</span>\r\n                  <div class=\"text-light mt-2 text-sm\">Your current balance</div>\r\n                  <div>\r\n                    <span class=\"text-success font-weight-600\">+ 15%</span> <span class=\"text-light\">($250)</span>\r\n                  </div>\r\n                </div>\r\n                <button class=\"btn btn-sm btn-block btn-neutral\">Add credit</button>\r\n              </div>\r\n\r\n              <div class=\"card-body\">\r\n                <div class=\"row\">\r\n                  <div class=\"col\">\r\n                    <small class=\"text-light\">Orders: 60%</small>\r\n                    <base-progress :value=\"60\"\r\n                                   size=\"xs\"\r\n                                   progress-classes=\"my-2\"\r\n                                   type=\"success\"/>\r\n                  </div>\r\n                  <div class=\"col\">\r\n                    <small class=\"text-light\">Sales: 40%</small>\r\n                    <base-progress :value=\"40\"\r\n                                   size=\"xs\"\r\n                                   progress-classes=\"my-2\"\r\n                                   type=\"warning\"/>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n\r\n            </card>\r\n\r\n            <card gradient=\"danger\">\r\n\r\n              <div class=\"row justify-content-between align-items-center\">\r\n                <div class=\"col\">\r\n                  <img src=\"img/icons/cards/bitcoin.png\" alt=\"Image placeholder\">\r\n                </div>\r\n                <div class=\"col-auto\">\r\n                  <span class=\"badge badge-lg badge-success\">Active</span>\r\n                </div>\r\n              </div>\r\n              <div class=\"my-4\">\r\n                  <span class=\"h6 surtitle text-light\">\r\n                    Username\r\n                  </span>\r\n                <div class=\"h1 text-white\">@johnsnow</div>\r\n              </div>\r\n              <div class=\"row\">\r\n                <div class=\"col\">\r\n                  <span class=\"h6 surtitle text-light\">Name</span>\r\n                  <span class=\"d-block h3 text-white\">John Snow</span>\r\n                </div>\r\n              </div>\r\n\r\n            </card>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <!--End Widgets-->\r\n\r\n      <!--Tables-->\r\n      <div class=\"row\">\r\n        <div class=\"col-xl-8\">\r\n          <page-visits-table></page-visits-table>\r\n        </div>\r\n        <div class=\"col-xl-4\">\r\n          <social-traffic-table></social-traffic-table>\r\n        </div>\r\n      </div>\r\n      <!--End tables-->\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n<script>\r\n  // Charts\r\n  import * as chartConfigs from '@/components/Charts/config';\r\n  import LineChart from '@/components/Charts/LineChart';\r\n  import BarChart from '@/components/Charts/BarChart';\r\n\r\n  // Components\r\n  import BaseProgress from '@/components/BaseProgress';\r\n  import RouteBreadCrumb from '@/components/Breadcrumb/RouteBreadcrumb';\r\n  import StatsCard from '@/components/Cards/StatsCard';\r\n\r\n  // Lists\r\n  import ActivityFeed from './ActivityFeed';\r\n  import TaskList from './TaskList';\r\n  import UserList from './UserList';\r\n  import ProgressTrackList from './ProgressTrackList';\r\n\r\n  // Tables\r\n  import LightTable from './LightTable';\r\n  import SocialTrafficTable from './SocialTrafficTable';\r\n  import PageVisitsTable from './PageVisitsTable';\r\n\r\n  export default {\r\n    components: {\r\n      ActivityFeed,\r\n      LineChart,\r\n      BarChart,\r\n      BaseProgress,\r\n      RouteBreadCrumb,\r\n      StatsCard,\r\n      TaskList,\r\n      PageVisitsTable,\r\n      SocialTrafficTable,\r\n      LightTable,\r\n      UserList,\r\n      ProgressTrackList\r\n    },\r\n    data() {\r\n      return {\r\n        bigLineChart: {\r\n          allData: [\r\n            [0, 20, 10, 30, 15, 40, 20, 60, 60],\r\n            [0, 20, 5, 25, 10, 30, 15, 40, 40]\r\n          ],\r\n          activeIndex: 0,\r\n          chartData: {\r\n            datasets: [\r\n              {\r\n                label: 'Performance',\r\n                data: [0, 20, 10, 30, 15, 40, 20, 60, 60],\r\n              }\r\n            ],\r\n            labels: ['May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'],\r\n          },\r\n          extraOptions: chartConfigs.blueChartOptions,\r\n        },\r\n        redBarChart: {\r\n          chartData: {\r\n            labels: ['Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'],\r\n            datasets: [{\r\n              label: 'Sales',\r\n              data: [25, 20, 30, 22, 17, 29]\r\n            }]\r\n          },\r\n          extraOptions: chartConfigs.blueChartOptions\r\n        }\r\n      };\r\n    },\r\n    methods: {\r\n      initBigChart(index) {\r\n        let chartData = {\r\n          datasets: [\r\n            {\r\n              label: 'Performance',\r\n              data: this.bigLineChart.allData[index]\r\n            }\r\n          ],\r\n          labels: ['May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'],\r\n        };\r\n        this.bigLineChart.chartData = chartData;\r\n        this.bigLineChart.activeIndex = index;\r\n      }\r\n    },\r\n    mounted() {\r\n      this.initBigChart(0);\r\n    }\r\n  };\r\n</script>\r\n<style></style>\r\n"],"sourceRoot":"src/views/Dashboard"}]}