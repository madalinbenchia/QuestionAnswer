A","sourcesContent":["<template>\r\n  <div class=\"custom-file\">\r\n    <input type=\"file\"\r\n           class=\"custom-file-input\"\r\n           id=\"customFileLang\"\r\n           lang=\"en\"\r\n           v-bind=\"$attrs\"\r\n           v-on=\"listeners\"\r\n    />\r\n    <label class=\"custom-file-label\" for=\"customFileLang\">\r\n      {{label}}\r\n    </label>\r\n  </div>\r\n</template>\r\n<script>\r\n  export default {\r\n    name: 'file-input',\r\n    inheritAttrs: false,\r\n    props: {\r\n      initialLabel: {\r\n        type: String,\r\n        default: 'Select file'\r\n      }\r\n    },\r\n    data() {\r\n      return {\r\n        files: []\r\n      }\r\n    },\r\n    computed: {\r\n      listeners() {\r\n        return {\r\n          ...this.$listeners,\r\n          change: this.fileChange\r\n        }\r\n      },\r\n      label() {\r\n        let fileNames = [];\r\n        for (let file of this.files) {\r\n          fileNames.push(file.name)\r\n        }\r\n        return fileNames.length ? fileNames.join(', ') : this.initialLabel\r\n      }\r\n    },\r\n    methods: {\r\n      fileChange(evt) {\r\n        this.files = evt.target.files\r\n        this.$emit('change', this.files)\r\n      }\r\n    }\r\n  }\r\n</script>\r\n<style>\r\n</style>\r\n"],"sourceRoot":"src/components/Inputs"}]}