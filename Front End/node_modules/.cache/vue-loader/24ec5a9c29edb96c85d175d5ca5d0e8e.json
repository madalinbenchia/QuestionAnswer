    $route: {\r\n        immediate: true,\r\n        handler: function () {\r\n          this.updateBackground()\r\n        }\r\n      }\r\n    }\r\n  };\r\n</script>\r\n<style lang=\"scss\">\r\n  $scaleSize: 0.8;\r\n  @keyframes zoomIn8 {\r\n    from {\r\n      opacity: 0;\r\n      transform: scale3d($scaleSize, $scaleSize, $scaleSize);\r\n    }\r\n    100% {\r\n      opacity: 1;\r\n    }\r\n  }\r\n\r\n  .main-content .zoomIn {\r\n    animation-name: zoomIn8;\r\n  }\r\n\r\n  @keyframes zoomOut8 {\r\n    from {\r\n      opacity: 1;\r\n    }\r\n    to {\r\n      opacity: 0;\r\n      transform: scale3d($scaleSize, $scaleSize, $scaleSize);\r\n    }\r\n  }\r\n\r\n  .main-content .zoomOut {\r\n    animation-name: zoomOut8;\r\n  }\r\n</style>\r\n"]}]}